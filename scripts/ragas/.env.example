# RAGAS evaluation env template
# Copy to scripts/ragas/.env.local and fill values.

# Required: access token cookie value (JWT only, without "access_token=" prefix)
PKV_ACCESS_TOKEN=

# Optional: explicit member id (if omitted, parsed from JWT sub)
# PKV_MEMBER_ID=

# Judge model (optional)
RAGAS_JUDGE_MODEL=gpt-4o-mini

# API base URL
PKV_BASE_URL=http://localhost:8080

# Preferred: dedicated evaluation key
# RAGAS_OPENAI_API_KEY=

# Reuse existing app key (fallback if RAGAS_OPENAI_API_KEY is empty)
# CHAT_MODEL_API_KEY=

# MySQL (read-only queries)
DB_HOST=localhost
DB_PORT=3306
DB_NAME=pkv
DB_USERNAME=root
DB_PASSWORD=root

# Qdrant
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION=pkv_text_segments
